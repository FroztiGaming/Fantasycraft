{
  "schemaVersion": 1,
  "id": "fantasycraft",
  "version": "${version}",

  "name": "Fantasycraft",
  "description": "Fantasycraft is an add-on for the Origins mod",
  "authors": [
    "FroztiGaming"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FroztiGaming/Fantasycraft"
  },

  "license": "MIT",
  "icon": "assets/fantasycraft/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
